{
    "name": "nest-babel-starter",
    "version": "1.0.0",
    "description": "Nest Babel starter repository",
    "license": "MIT",
    "scripts": {
        "format": "prettier --write \"**/*.js\"",
        "clean": "rimraf dist",
        "lintfix": "eslint src --fix",
        "start": "babel-node index.js",
        "transpile": "babel src --out-dir dist --copy-files",
        "build": "npm run clean && npm run transpile",
        "seed": "node ./src/seeders/index.js",
        "dev": "nodemon --watch src --exec \"eslint src --fix && babel-node\" index.js",
        "local": "nodemon --watch src --exec \"babel-node\" index.js"
    },
    "dependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/mongoose": "^11.0.1",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/schedule": "^4.0.1",
        "@sentry/node": "^7.112.0",
        "@sentry/tracing": "^7.112.0",
        "adm-zip": "^0.5.12",
        "body-parser": "^1.20.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "crypto-js": "^4.2.0",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.0",
        "handlebars": "^4.7.8",
        "heic-convert": "^2.1.0",
        "lodash": "^4.17.21",
        "md5": "^2.3.0",
        "mongoose": "^8.9.6",
        "mysql2": "^3.9.2",
        "nodemailer": "^6.9.13",
        "onesignal-node": "^3.4.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.5.5",
        "sharp": "^0.33.5",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.0",
        "@babel/core": "^7.18.6",
        "@babel/node": "^7.18.6",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.24.1",
        "@babel/plugin-transform-runtime": "^7.18.6",
        "@babel/preset-env": "^7.18.6",
        "@babel/register": "^7.18.6",
        "@babel/runtime": "^7.18.6",
        "@nestjs/testing": "^10.0.0",
        "@types/passport-local": "^1.0.38",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-minify": "^0.5.1",
        "eslint": "^6.8.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-cypress": "^2.11.3",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-lodash": "^7.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-promise": "^4.3.1",
        "faker": "^5.4.0",
        "husky": "^2.6.0",
        "jest": "^28.1.2",
        "lint-staged": "^8.2.1",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "sequelize-cli": "^6.2.0",
        "sequelize-log-syntax-colors": "^2.1.3-0",
        "supertest": "^6.2.4",
        "webpack": "^5.24.2",
        "webpack-cli": "^4.5.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json"
        ],
        "rootDir": "src",
        "testRegex": ".spec.js$",
        "coverageDirectory": "../coverage"
    }
}